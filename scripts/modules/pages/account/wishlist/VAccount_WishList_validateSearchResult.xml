<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Validates the result of a wishlist search</description>
  <parameter name="firstName"/>
  <parameter name="lastName"/>
  <command name="assertText" target="css=#primary.primary-content table.item-list tbody tr th:nth-of-type(1)" value="regexpi:Last Name"/>
  <command name="assertText" target="css=#primary.primary-content table.item-list tbody tr th:nth-of-type(2)" value="regexpi:First Name"/>
  <command name="assertText" target="css=#primary.primary-content table.item-list tbody tr th:nth-of-type(3)" value="regexpi:Location"/>
  <command name="assertText" target="css=#primary.primary-content table.item-list tbody tr td.last-name" value="@{lastName}"/>
  <command name="assertText" target="css=#primary.primary-content table.item-list tbody tr td.first-name" value="@{firstName}"/>
</scriptmodule>